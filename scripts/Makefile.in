# Here also go all build targets specific to this very newgrf

GENERATE_NML := fish-nml
GENERATE_LNG := fish-nml

GENERATE=src/build_fish.py

fish-nml: src/FISH.cfg $(GENERATE)
	$(_E) [GENERATE] from FISH.cfg
	$(_V) python src/build_fish.py '${REPO_TITLE}' '${REPO_REVISION}'

clean::
	$(_V)-rm fish.nml
	$(_V)-rm lang/*.lng
